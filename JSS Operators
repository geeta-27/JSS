<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
         x = 10;
         y = 3;

         var sum = x+y;
         var sub = x-y;
         var mul = x*y;
         var div = x/y;
         var exp = x**y;
         var mod = x%y;


        
        console.log("sum: "+sum);
        console.log("sub: "+sub);
        console.log("mul: "+mul);
        console.log("div: "+div);
        console.log("exp: "+exp);
        console.log("mod: "+mod);

        console.log(++x);
        console.log(x++);
        console.log(--x);
        console.log(x--);
    </script>
    
</body>
</html>