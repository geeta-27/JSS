    <h1>Non-return type</h1>
    <script>
        //here it is not accessable or use

        function queen() {
            let var4 = "Amongus";
            document.write(var4);

            //here it is accessable or use
        }

        //here it is not accessable or use
    </script>
    <h1>retrun type</h1>
    <script>
        function queen() {
            let var4 = "Amongus";
            return var4;
        }
        document.write(queen());

    </script>


